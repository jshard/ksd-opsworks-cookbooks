#
# component profile for the <%= @vname %> component
#

<%= @name.upcase %>_HOME=<%= @vhome %>
<%= @name.upcase %>_CONFIG=<%= @vconfig %>
<%= @name.upcase %>_BASE=<%= @base %>
<%= @name.upcase %>_LOGS=<%= @logs %>
export <%= @name.upcase %>_HOME <%= @name.upcase %>_CONFIG <%= @name.upcase %>_BASE <%= @name.upcase %>_LOGS

ORACLE_HOME=$<%= @name.upcase %>_HOME
export ORACLE_HOME

PATH=$ORACLE_HOME:$PATH
export PATH
